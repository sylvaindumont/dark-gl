{
  "manifest_version": 2,

  "name": "black.gl",
  "description": "Black Google",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
	{
	  "matches": ["https://www.google.fr/*",
	  "https://www.youtube.com/*",
	  "https://translate.google.fr/*"
	  ],
	  "css": ["black.css"]
	}
  ],
  "permissions": [
    "https://www.google.fr/",
	"https://www.youtube.com/",
	"https://translate.google.fr/"
	]
}
